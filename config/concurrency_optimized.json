{
  "system": {
    "name": "AI Trading Bot - Concurrency Optimized",
    "description": "Optimized concurrency settings to reduce thread count and idle CPU usage",
    "max_concurrent_tasks": 4,
    "thread_pool_size": 2,
    "async_timeout": 15.0,
    "task_batch_size": 2,
    "update_frequency_seconds": 45.0
  },
  "scheduler": {
    "max_concurrent_tasks": 4,
    "task_timeout_seconds": 45,
    "batch_size": 2,
    "health_check_interval_seconds": 300,
    "error_recovery_delay_seconds": 30
  },
  "websocket": {
    "queue_size": 200,
    "max_retries": 10,
    "retry_delay": 5.0,
    "ping_interval": 30.0,
    "ping_timeout": 15.0,
    "health_check_interval": 60.0,
    "connection_timeout": 30.0
  },
  "io_effects": {
    "parallel_max_workers": 2,
    "thread_pool_optimization": true,
    "executor_shutdown_timeout": 10.0
  },
  "market_data": {
    "subscriber_thread_pool_size": 2,
    "notification_batch_size": 5,
    "cache_cleanup_interval": 300
  },
  "validation": {
    "max_concurrent_validations": 2,
    "validation_timeout": 10.0,
    "thread_pool_size": 2
  },
  "monitoring": {
    "check_interval": 60,
    "metrics_collection_interval": 30,
    "thread_pool_size": 1
  },
  "exchange": {
    "rate_limit_requests": 8,
    "rate_limit_window_seconds": 60,
    "connection_pool_size": 2,
    "timeout_seconds": 15
  },
  "functional_programming": {
    "max_concurrent_effects": 25,
    "effect_timeout": 20.0,
    "interpreter_thread_pool_size": 2,
    "scheduler_enabled": true,
    "async_runtime": true
  },
  "resource_management": {
    "memory_cleanup_interval": 600,
    "gc_collection_interval": 300,
    "idle_connection_timeout": 180,
    "connection_pool_cleanup": true
  },
  "logging": {
    "async_logging": true,
    "log_buffer_size": 100,
    "log_flush_interval": 5.0,
    "max_log_handlers": 3
  }
}
