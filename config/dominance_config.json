{
  "trading": {
    "symbol": "BTC-USD",
    "interval": "1m",
    "max_size_pct": 20.0,
    "leverage": 3,
    "enable_futures": true,
    "futures_symbol": "BTC-USDT-PERP",
    "max_futures_leverage": 5,
    "auto_cash_transfer": true
  },
  "llm": {
    "provider": "openai",
    "model_name": "gpt-4",
    "temperature": 0.3,
    "max_tokens": 512,
    "top_p": 0.95,
    "frequency_penalty": 0.1,
    "presence_penalty": 0.1,
    "request_timeout": 30,
    "retry_attempts": 3,
    "retry_delay": 2
  },
  "risk": {
    "max_size_pct": 20.0,
    "max_daily_loss_pct": 5.0,
    "max_weekly_loss_pct": 15.0,
    "max_monthly_loss_pct": 30.0,
    "max_concurrent_trades": 2,
    "max_position_hold_hours": 24,
    "default_stop_loss_pct": 2.0,
    "default_take_profit_pct": 4.0,
    "min_account_balance": "500",
    "emergency_stop_loss_pct": 10.0
  },
  "dominance": {
    "enable_dominance_data": true,
    "data_source": "coingecko",
    "update_interval": 300,
    "dominance_weight_in_decisions": 0.25,
    "dominance_alert_threshold": 12.0,
    "dominance_change_alert_threshold": 1.0
  },
  "data": {
    "candle_limit": 200,
    "real_time_updates": true,
    "data_cache_ttl_seconds": 30,
    "indicator_warmup": 50,
    "cipher_a_ema_length": 21,
    "cipher_b_vwap_length": 20,
    "data_storage_path": "data",
    "keep_historical_days": 30
  },
  "system": {
    "dry_run": true,
    "environment": "development",
    "log_level": "INFO",
    "log_to_console": true,
    "log_to_file": true,
    "update_frequency_seconds": 60.0,
    "parallel_processing": true,
    "enable_monitoring": true,
    "enable_metrics": true
  }
}
