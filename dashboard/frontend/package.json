{
  "name": "ai-trading-bot-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "AI Trading Bot Dashboard Frontend",
  "scripts": {
    "dev": "vite --force",
    "build": "tsc && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:performance": "NODE_ENV=production npm run build && npm run analyze:bundle",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "analyze:bundle": "du -sh dist/* | sort -h",
    "clean": "rm -rf dist node_modules/.vite",
    "optimize": "npm run clean && npm run build:performance"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "~5.2.0",
    "vite": "^4.5.5",
    "esbuild": "^0.21.5",
    "@rollup/rollup-linux-arm64-musl": "4.9.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-arm64-musl": "4.9.5",
    "@rollup/rollup-linux-x64-musl": "4.9.5"
  },
  "overrides": {
    "esbuild": "^0.21.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
