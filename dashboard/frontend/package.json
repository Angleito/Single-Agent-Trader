{
  "name": "ai-trading-bot-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "AI Trading Bot Dashboard Frontend with Enterprise Phase 4 Features",
  "scripts": {
    "dev": "vite --force",
    "build": "vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:performance": "NODE_ENV=production npm run build && npm run analyze:bundle",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "analyze:bundle": "du -sh dist/* | sort -h",
    "clean": "rm -rf dist node_modules/.vite",
    "optimize": "npm run clean && npm run build:performance",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,json,css}",
    "format:check": "prettier --check src/**/*.{ts,tsx,json,css}"
  },
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "idb": "^8.0.0",
    "localforage": "^1.10.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/ui": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "typescript": "~5.2.0",
    "vite": "^4.5.5",
    "vite-plugin-pwa": "^0.20.0",
    "vitest": "^1.6.0",
    "esbuild": "^0.21.5",
    "@rollup/rollup-linux-arm64-musl": "4.9.5",
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-arm64-musl": "4.9.5",
    "@rollup/rollup-linux-x64-musl": "4.9.5"
  },
  "overrides": {
    "esbuild": "^0.21.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
